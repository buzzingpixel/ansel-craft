{
    "name": "buzzingpixel/ansel-craft",
    "version": "2.0.0-beta3",
    "type": "craft-plugin",
    "description": "A Craft CMS field type for defining image requirements and non-destructive editing of images per-field.",
    "license": "proprietary",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "Ansel for Craft"
    ],
    "authors": [
        {
            "name": "TJ Draper",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "buzzingpixel\\ansel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "felicity-php/felicity-datamodel": "^1.0",
        "ramsey/uuid": "^3.7",
        "samayo/bulletproof": "^3.2",
        "mrclay/minify": "^3.0",
        "pixelandtonic/imagine": "^0.7.1.2",
        "ps/image-optimizer": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "extra": {
        "name": "Ansel for Craft",
        "handle": "ansel",
        "developer": "BuzzingPixel, LLC",
        "developerUrl": "https://buzzingpixel.com",
        "documentationUrl": "https://buzzingpixel.com/software/ansel-craft/documentation",
        "changelogUrl": "https://github.com/buzzingpixel/ansel-craft/blob/master/changelog.md",
        "schemaVersion": "2.0.0",
        "hasCpSettings": true,
        "class": "buzzingpixel\\ansel\\Ansel"
    }
}
