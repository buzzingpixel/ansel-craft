        </div>
        <div class="spinner big AnselField__Spinner"></div>
        <div class="AnselField__Inner">
            <div class="AnselField__DropImagesToUpload">
                {{ 'Drag images here to upload'|t('app')|anselWidont }}
            </div>
            <div class="AnselField__ImagesHolder">
                {% for i in 1..8 %}
                <div class="AnselField__Image">
                    <div class="AnselField__ImageIcons">
                        <div class="AnselField__ImageIconsInner">
                            <div class="AnselField__ImageIcon AnselField__ImageIconEdit">
