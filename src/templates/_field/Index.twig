{# @var string uploadKey #}
{# @var string uploadActionUrl #}
{# @var string csrfToken #}
{# @var \buzzingpixel\ansel\models\AnselFieldSettingsModel settings #}

<div
    class="AnselField JSAnselField"
    data-upload-key="{{ uploadKey }}"
    data-upload-action-url="{{ uploadActionUrl }}"
    data-csrf-token="{{ csrfToken }}"
    data-settings='{{ settings.asArray(true)|json_encode }}'
>
    <ul class="AnselField__Notifications JSAnselField__Notifications"></ul>
    <div class="AnselField__MainWrap">
